<h3>Clients</h3>
<table class="table table-bordered">
    <tr>
        <th class="table-bordered th">Client Name</th>
    </tr>
    @foreach (var item in (List<AssClassLibrary.Client>)ViewData["clientResults"])
    {
        <tr>
            <td>
                @Html.ActionLink(item.Name, "", new { id = item.ClientId })
            </td>
        </tr>
    }
    <tr>
        <th class="table-bordered th">Project Name</th>
    </tr>
    @foreach (var item in (List<AssClassLibrary.Project>)ViewData["projectResults"])
    {
        <tr>
            <td>
                @Html.ActionLink(item.Name, "", new { id = item.ClientId })
            </td>
        </tr>
    }
    <tr>
        <th class="table-bordered th">Entry Name</th>
    </tr>
    @foreach (var item in (List<AssClassLibrary.Entry>)ViewData["entryResults"])
    {
        <tr>
            <td>
                @Html.ActionLink(item.Username, "", new { id = item.ProjectId })
            </td>
        </tr>
    }
</table>