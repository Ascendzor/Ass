﻿<!DOCTYPE html>
<html>
<head>
    <title>Layout Page Content</title>
    <link href="/Content/Site.css" rel="Stylesheet" type="text/css" />
    <link href="~/Content/bootstrap/css/bootstrap.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.7.1.min.js"></script>
    <script src="~/Scripts/Home.js"></script>
    <script src="~/Scripts/KeyboardNav.js"></script>
</head>
<body>
    
	<div class="navbar navbar-inverse navbar-fixed-top navbar-inner" style="min-height:30px">
		<div class="mainTitle"><a onclick="window.location.reload();">
			<i class="icon-large icon-white icon-home" style="margin-right:3px"></i></a> Authentication Storage System
			<input id="searchText" oninput="search()" name="searchtext" type="text" autocomplete="off"/>
			<button id="myButton" style="margin-bottom:5px; margin-left:5px" onclick="search(); $('#searchText').focus();" class="btn btn-default navbar-btn" >Go</button>
			<!-- Traffic Light -->
            <input checked="checked" type="checkbox" id="Development" onclick="filterByDevState('Development')"/>
            <input checked="checked" type="checkbox" id="Testing" onclick="filterByDevState('Testing')"/>
            <input checked="checked" type="checkbox" id="Staging" onclick="filterByDevState('Staging')"/>
		</div>
	</div>	
	<div class="containerDiv">
        <div class="contentDiv">
            <div id="errorDiv"></div>
            <!-- Dynamic content placed in here -->
            <div id="partialDiv"></div>
        </div>
    </div>
</body>
</html>
